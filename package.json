{
  "name": "Azurite",
  "version": "0.1.0",
  "description": "Casual Painting App in Electron",
  "main": "dist/assets/main.js",
  "scripts": {
    "build:autogypi": "autogypi",
    "node-gyp:rebuild": "node-gyp rebuild --target=1.5.0 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "node-gyp:build": "node-gyp build --target=1.5.0 --arch=x64 --dist-url=https://atom.io/download/atom-shell",
    "build:nativelib": "npm-run-all build:autogypi node-gyp:rebuild",
    "build:main": "webpack --config webpack.config.main.js",
    "build:renderer": "webpack",
    "build": "npm-run-all build:main build:renderer",
    "watch:main": "webpack --watch --config webpack.config.main.js",
    "devserver": "webpack-dev-server --hot",
    "watch": "npm-run-all --parallel watch:main devserver",
    "app": "cross-env NODE_ENV=development electron . --devserver",
    "start": "npm-run-all build --parallel devserver app",
    "lint": "tslint 'src/**/*.{ts,tsx}'",
    "test:dev": "cross-env NODE_ENV=test electron . --devserver",
    "pretest": "npm run build",
    "test": "cross-env NODE_ENV=test electron .",
    "install": "npm run build:nativelib",
    "postinstall": "electron-rebuild -e node_modules/electron -w receive-tablet-event",
    "package": "ts-node scripts/package.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanchas116/azurite.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/seanchas116/azurite/issues"
  },
  "homepage": "https://github.com/seanchas116/azurite#readme",
  "private": true,
  "devDependencies": {
    "@types/classnames": "0.0.32",
    "@types/electron": "^1.4.31",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/mocha": "^2.2.38",
    "@types/msgpack-lite": "^0.1.2",
    "@types/node": "^7.0.4",
    "@types/power-assert": "^1.4.29",
    "@types/react": "^15.0.3",
    "@types/react-dom": "^0.14.21",
    "autogypi": "^0.2.2",
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-plugin-espower": "^2.3.2",
    "babel-plugin-glslify": "^2.0.0",
    "cross-env": "^3.1.4",
    "css-loader": "^0.26.1",
    "electron": "^1.5.0",
    "electron-devtools-installer": "^2.0.1",
    "electron-packager": "^8.5.1",
    "electron-rebuild": "^1.5.7",
    "file-loader": "^0.9.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.2.0",
    "mocha-loader": "^1.1.0",
    "nbind": "^0.3.9",
    "node-gyp": "^3.5.0",
    "npm-run-all": "^4.0.1",
    "postcss-cssnext": "^2.9.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.2.2",
    "postcss-url": "^5.1.2",
    "power-assert": "^1.4.2",
    "resolve": "^1.2.0",
    "shelljs": "^0.7.6",
    "stack-source-map": "^1.0.6",
    "style-loader": "^0.13.1",
    "ts-loader": "^2.0.0",
    "ts-node": "^2.0.0",
    "tslint": "^4.3.1",
    "typescript": "^2.1.5",
    "url-loader": "^0.5.7",
    "webpack": "^1.14.0",
    "webpack-dev-server": "^1.16.2",
    "webpack-fail-plugin": "^1.0.5"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "decamelize": "^1.2.0",
    "deep-assign": "^2.0.0",
    "deep-equal": "^1.0.1",
    "glslify": "^6.0.1",
    "minimist": "^1.2.0",
    "mobx": "^3.0.2",
    "mobx-react": "^4.1.0",
    "msgpack-lite": "^0.1.26",
    "paintgl": "github:sketchglass/paintgl#64926a07ab5eadbc97845ec417e1e2acdfc35714",
    "paintvec": "github:sketchglass/paintvec#2c5deb2c4cea6e2b22e43cb5bf8b96115dc97455",
    "promise-semaphore": "^0.2.8",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-draggable-tree": "sketchglass/react-draggable-tree#398cbfcb0c58088b865835faca1f6306603ac754",
    "receive-tablet-event": "github:seanchas116/receive-tablet-event#8147b9191c67fb6c818ff9c6eab28e0e6fb699b7"
  }
}
