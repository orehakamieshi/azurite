{
  "name": "Azurite",
  "version": "0.1.0",
  "description": "Casual Painting App in Electron",
  "main": "dist/assets/main.js",
  "scripts": {
    "build:main": "webpack --config webpack.config.main.js",
    "build:renderer": "webpack",
    "build": "npm-run-all build:main build:renderer",
    "watch:main": "webpack --watch --config webpack.config.main.js",
    "devserver": "webpack-dev-server --hot",
    "watch": "npm-run-all --parallel watch:main devserver",
    "app": "cross-env NODE_ENV=development electron .",
    "start": "npm-run-all build --parallel devserver app",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "electron-rebuild -e node_modules/electron -w receive-tablet-event",
    "package": "ts-node scripts/package.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seanchas116/azurite.git"
  },
  "author": "",
  "bugs": {
    "url": "https://github.com/seanchas116/azurite/issues"
  },
  "homepage": "https://github.com/seanchas116/azurite#readme",
  "private": true,
  "devDependencies": {
    "@types/electron": "^1.3.22",
    "@types/msgpack-lite": "^0.1.1",
    "@types/node": "^6.0.41",
    "@types/react": "^0.14.37",
    "@types/react-dom": "^0.14.17",
    "cross-env": "^3.1.3",
    "css-loader": "^0.25.0",
    "electron": "^1.4.1",
    "electron-packager": "^8.1.0",
    "electron-rebuild": "^1.2.1",
    "file-loader": "^0.9.0",
    "npm-run-all": "^3.1.0",
    "postcss-cssnext": "^2.8.0",
    "postcss-import": "^8.1.2",
    "postcss-loader": "^0.13.0",
    "postcss-url": "^5.1.2",
    "shelljs": "^0.7.4",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "ts-node": "^1.3.0",
    "typescript": "^2.0.3",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "mobx": "^2.6.0",
    "mobx-react": "^3.5.8",
    "msgpack-lite": "^0.1.26",
    "paintgl": "github:sketchglass/paintgl#27ecdbde87d95bda0b055b4a173e5ad39a4cd0fc",
    "paintvec": "github:sketchglass/paintvec#c5f1efe8db19fcd135a3661799c652dd5beb1427",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-draggable-tree": "sketchglass/react-draggable-tree#a375224f0b5637ce50459109dbbf345c53901e91",
    "receive-tablet-event": "github:seanchas116/receive-tablet-event#72ea99e935894131c3632380b12f23ee159c4c18",
    "rxjs": "^5.0.0-rc.1"
  }
}
